
<% include ../partials/header %>

<h1>Registrate como instructora!</h1>

<form action="/register/instructora" method="POST">
	<input type="text" name="nombre" placeholder="Nombre completo">
  <input type="text" name="email" placeholder="Email">
	<input type="password" name="password" placeholder="Password">
  <input type="date" name="fechaDeNacimiento" placeholder="Fecha de Nacimiento">
  <input type="number" name="telefono" placeholder="Telefono">
  <input type="text" name="sedePreferencia" placeholder="Sedes de preferencia">
	<button>Sign Up</button>	
</form>

